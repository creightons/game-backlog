{
  "name": "game-backlog",
  "version": "1.0.0",
  "description": "Practice app to create a backlog of games",
  "main": "server.js",
  "scripts": {
    "build-js": "browserify  -t [ babelify --presets [ es2015 ] ] frontend/js/index > public/build.js",
    "watch-js": "watchify  -dv -t [ babelify --presets [ es2015 ] ] frontend/js/index -o public/build.js",
    "build-css": "node-sass frontend/scss/index.scss public/index.css",
    "watch-css": "npm run build-css && npm run build-css -- -w",
    "watch": "concurrently \"npm run watch-css\" \"npm run watch-js\" ",
    "build-react-js": "browserify  -t [ babelify --presets [ es2015 react ] ] frontend-react/js/index > public/build.js",
    "watch-react-js": "watchify  -dv -t [ babelify --presets [ es2015 react ] ] frontend-react/js/index -o public/build.js",
    "build-react-css": "node-sass frontend-react/scss/index.scss public/index.css",
    "watch-react-css": "npm run build-react-css && npm run build-react-css -- -w",
    "watch-react": "concurrently \"npm run watch-react-css\" \"npm run watch-react-js\" "
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "browserify": "^14.1.0",
    "concurrently": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-util": "^3.0.8",
    "igdb-api-node": "^2.0.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash.merge": "^4.6.0",
    "mongoose": "^4.8.3",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pubsub-js": "^1.5.4",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "devDependencies": {},
  "author": "",
  "license": "ISC"
}
